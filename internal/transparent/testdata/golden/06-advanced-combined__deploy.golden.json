{
  "global_vars": [
    {
      "name": "ALIAS",
      "origin": "special",
      "type": "string",
      "value": "deploy"
    },
    {
      "name": "ROOT_DIR",
      "origin": "special",
      "type": "string",
      "value": "<PATH>"
    },
    {
      "name": "ROOT_TASKFILE",
      "origin": "special",
      "type": "string",
      "value": "<PATH>"
    },
    {
      "name": "TASK",
      "origin": "special",
      "type": "string",
      "value": "deploy"
    },
    {
      "name": "TASKFILE",
      "origin": "special",
      "type": "string",
      "value": "<PATH>"
    },
    {
      "name": "TASKFILE_DIR",
      "origin": "special",
      "type": "string",
      "value": "<PATH>"
    },
    {
      "name": "TASK_DIR",
      "origin": "special",
      "type": "string",
      "value": "<PATH>"
    },
    {
      "name": "TASK_EXE",
      "origin": "special",
      "type": "string",
      "value": "<PATH>"
    },
    {
      "name": "TASK_VERSION",
      "origin": "special",
      "type": "string",
      "value": "<TASK_VERSION>"
    },
    {
      "name": "USER_WORKING_DIR",
      "origin": "special",
      "type": "string",
      "value": "<PATH>"
    },
    {
      "is_dynamic": true,
      "name": "BUILD_DATE",
      "origin": "taskfile:vars",
      "sh_cmd": "echo \"2026-02-11\"",
      "type": "string",
      "value": "2026-02-11"
    },
    {
      "name": "PROJECT",
      "origin": "taskfile:vars",
      "type": "string",
      "value": "myproject"
    },
    {
      "name": "VERSION",
      "origin": "taskfile:vars",
      "type": "string",
      "value": "2.0.0"
    }
  ],
  "tasks": [
    {
      "commands": [
        {
          "index": 0,
          "raw": "echo 'Deploying {{.PROJECT}} to {{.TARGET}}'",
          "resolved": "echo 'Deploying myproject to production'"
        }
      ],
      "dependencies": [
        "build"
      ],
      "name": "deploy",
      "templates": [
        {
          "context": "cmds[0]",
          "input": "echo 'Deploying {{.PROJECT}} to {{.TARGET}}'",
          "output": "echo 'Deploying myproject to production'",
          "vars_used": [
            "PROJECT",
            "TARGET"
          ]
        }
      ],
      "variables": [
        {
          "name": "MATCH",
          "origin": "call:vars",
          "type": "[]string",
          "value": [],
          "value_id": 0
        },
        {
          "name": "TARGET",
          "origin": "task:vars",
          "type": "string",
          "value": "production"
        }
      ]
    }
  ],
  "version": "1.0"
}
