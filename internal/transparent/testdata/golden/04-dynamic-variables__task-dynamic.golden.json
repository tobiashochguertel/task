  "tasks": [
    {
      "name": "task-dynamic",
      "variables": [
        {
          "name": "MATCH",
          "origin": "call:vars",
          "type": "[]string",
          "value": [],
          "value_id": 0
        },
        {
          "name": "COMBINED",
          "origin": "task:vars",
          "type": "string",
          "value": "test-host-task-123"
        },
        {
          "name": "TASK_ID",
          "origin": "task:vars",
          "type": "string",
          "value": "task-123",
          "is_dynamic": true,
          "sh_cmd": "echo \"task-123\""
        }
      ],
      "templates": [
        {
          "input": "echo 'Task ID: {{.TASK_ID}}'",
          "output": "echo 'Task ID: task-123'",
          "context": "cmds[0]",
          "vars_used": [
            "TASK_ID"
          ]
        },
        {
          "input": "echo 'Combined: {{.COMBINED}}'",
          "output": "echo 'Combined: test-host-task-123'",
          "context": "cmds[1]",
          "vars_used": [
            "COMBINED"
          ]
        }
      ],
      "commands": [
        {
          "index": 0,
          "raw": "echo 'Task ID: {{.TASK_ID}}'",
          "resolved": "echo 'Task ID: task-123'"
        },
        {
          "index": 1,
          "raw": "echo 'Combined: {{.COMBINED}}'",
          "resolved": "echo 'Combined: test-host-task-123'"
        }
      ]
    }
